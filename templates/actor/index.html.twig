{% extends 'base.html.twig' %}

{% block title %}Hello ActorController!{% endblock %}

{% block body %}
    <h3>Liste des acteurs :</h3>
    {% for actor in actors %}
        <div>
            <a href="{{ path('actor_show', {id: actor.id}) }}">{{ actor.name }}</a>
        </div>
    {% endfor %}
{% endblock %}
